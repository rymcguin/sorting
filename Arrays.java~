import java.util.Random;
public class Arrays{
  public static void main(String[] args){
    Random rand = new Random();
    int [] arrayOneHundred = new int[100];
    int i;
    
    for(i = 0; i < 100; i++){
      arrayOneHundred [i] = rand.nextInt(100);
      System.out.print(arrayOneHundred[i] + ", " );
    }
    





  }
}

